    Sub Ini_Field()


        For i = 0 To 11
            For j = 0 To 11
                Field_General(i, j) = "@"
            Next
        Next

        For i = 1 To 10
            For j = 1 To 10
                Field_General(i, j) = "░"
            Next
        Next

        '  For y = 0 To 11
        ' For x = 0 To 11
        ' Console.Write(Field_General(x, y) & " ")
        ' Next
        '  Console.WriteLine("")
        ' Next


    End Sub



    Sub Draw_Fields()
        Call Refresh_Screen()

        SetConsoleColors(ConsoleColor.aqua)

        Console.Write("                                                  ╦ ╦╔═╗╦ ╦╦═╗  ╔╗ ╔═╗╔═╗╦═╗╔╦╗")
        SetConsoleColors(ConsoleColor.red)
        Console.Write("                                                              ╔═╗╦  ╔╗ ╔═╗╔═╗╦═╗╔╦╗")
        Console.WriteLine()
        SetConsoleColors(ConsoleColor.aqua)
        Console.Write("                                                  ╚╦╝║ ║║ ║╠╦╝  ╠╩╗║ ║╠═╣╠╦╝ ║║")
        SetConsoleColors(ConsoleColor.red)
        Console.Write("                                                              ╠═╣║  ╠╩╗║ ║╠═╣╠╦╝ ║║")
        Console.WriteLine()
        SetConsoleColors(ConsoleColor.aqua)
        Console.Write("                                                   ╩ ╚═╝╚═╝╩╚═  ╚═╝╚═╝╩ ╩╩╚══╩╝")
        SetConsoleColors(ConsoleColor.red)
        Console.Write("                                                              ╩ ╩╩  ╚═╝╚═╝╩ ╩╩╚══╩╝")
        Console.WriteLine()







        Console.WriteLine("")
        Console.Write("                                           ")
        SetConsoleColors(ConsoleColor.aqua)
        Console.Write("    A   B   C   D   E   F   G   H   I   J   ")
        Console.Write("                                           ")
        SetConsoleColors(ConsoleColor.red)
        Console.WriteLine("    A   B   C   D   E   F   G   H   I   J   ")

        Console.WriteLine("")

        If Player_Turn = True Then
            For j = 1 To 10
                If j < 10 Then
                    SetConsoleColors(ConsoleColor.aqua)
                    Console.Write("                                          " & "0" & j & "   ")
                Else
                    SetConsoleColors(ConsoleColor.aqua)
                    Console.Write("                                          " & j & "   ")
                End If

                For i = 1 To 10
                    SetConsoleColors(ConsoleColor.aqua)
                    If Ships_Placed = False Then
                        Console.Write(Field_General(i, j) & "   ")
                    Else
                        Console.Write(Field_Player(i, j) & "   ")
                    End If
                Next

                If j < 10 Then
                    SetConsoleColors(ConsoleColor.red)
                    Console.Write("                                          " & "0" & j & "   ")
                Else
                    SetConsoleColors(ConsoleColor.red)
                    Console.Write("                                          " & j & "   ")
                End If

                For i = 1 To 10
                    SetConsoleColors(ConsoleColor.red)
                    If Ships_Placed = False Then
                        Console.Write(Field_AI(i, j) & "   ")
                    Else
                        Console.Write(Field_General(i, j) & "   ")
                    End If
                Next


                Console.WriteLine("")
                Console.WriteLine("")
            Next

        Else

            For j = 1 To 10
                If j < 10 Then
                    SetConsoleColors(ConsoleColor.aqua)
                    Console.Write("                                          " & "0" & j & "   ")
                Else
                    SetConsoleColors(ConsoleColor.aqua)
                    Console.Write("                                          " & j & "   ")
                End If

                For i = 1 To 10
                    SetConsoleColors(ConsoleColor.aqua)
                    Console.Write(Field_Player(i, j) & "   ")
                Next

                If j < 10 Then
                    SetConsoleColors(ConsoleColor.red)
                    Console.Write("                                          " & "0" & j & "   ")
                Else
                    SetConsoleColors(ConsoleColor.red)
                    Console.Write("                                          " & j & "   ")
                End If

                For i = 1 To 10
                    SetConsoleColors(ConsoleColor.red)
                    Console.Write(Field_General(i, j) & "   ")
                Next


                Console.WriteLine("")
                Console.WriteLine("")
            Next
        End If

        SetConsoleColors(ConsoleColor.aqua)

        If Ships_Placed = True Then

            Console.Write("                                          Your Info")
            SetConsoleColors(ConsoleColor.red)
            Console.WriteLine("                                                                              Ai's Info")
            Console.WriteLine("")
            SetConsoleColors(ConsoleColor.aqua)
            Console.Write("                                          Last Target: " & "( " & X_Coordinate & " " & Y_Coordinate & " )")
            SetConsoleColors(ConsoleColor.red)
            Console.Write("                                                                   Last Target: " & "( " & Ai_X & " " & Ai_Y & " )")
            Console.WriteLine("")
            SetConsoleColors(ConsoleColor.aqua)
            Console.Write("                                          Points to win: " & 20 - Counter_Points_player)
            SetConsoleColors(ConsoleColor.red)
            Console.Write("                                                                      Points to win: " & 20 - Counter_Points_Ai)
            Console.WriteLine("")
        End If
        'Make a border (spacings) Write the line of letters $ABCDEFGHIJ with spaces in between, then write (spacings) and $ABCDEFGHIJ with spaces in between again
        'Make a border (spacings) Wtite 1 and first(top) line of the Field_Player with spaces in between then write (spacings) and repeat the same for Field_AI
        'Do the previous step increasing the number coordinate and outputing the lower lines
        Call Refresh_Screen()
    End Sub

    Sub Copy_Fields()

        If Player_Turn = True Then
            For i = 0 To 11
                For j = 0 To 11
                    Field_Player(i, j) = Field_General(i, j)
                Next
            Next
        Else
            For i = 0 To 11
                For j = 0 To 11
                    Field_AI(i, j) = Field_General(i, j)
                Next
            Next
        End If

    End Sub

    Sub Place_Ships()


        Dim Input_Check As Boolean = False
        Dim Ship_Szie As Integer = 4
        Dim Did_Clash As Boolean = False
        Dim Player_Case_Check As Boolean = False


        Do Until Input_Check = True And Counter_Ships = 0

            If Player_Turn = True Then
                Console.WriteLine("Please input the coordinates In the correct format (Letter Number Direction)")




                Input_Place = Console.ReadLine



                If Ship_Szie = 1 Then
                    If Input_Place.Length = 5 Then
                        Input_Place = Mid(Input_Place, 1, 3) + " U"
                    Else
                        Input_Place = Input_Place + " U"
                    End If
                End If
            End If

            Dim Input_Place_Split() As String = Input_Place.Split(New Char() {" "c})

            For Each item As Char In Input_Place_Split
                Amount_Of_The_Elements = Amount_Of_The_Elements + 1
            Next

            If Amount_Of_The_Elements > 1 Then
                If Input_Place_Split(0) = "George" And Input_Place_Split(1) = "Sandev" Then
                    Call Bulgaria()
                    Call Refresh_Screen()
                    Call Refresh_Screen()
                    Call Draw_Fields()
                End If
            End If

            If Amount_Of_The_Elements = 1 Then
                If Input_Place_Split(0) = "Chamomile" Then
                    Call Chamomile()
                    Call Refresh_Screen()
                    Call Refresh_Screen()
                    Call Draw_Fields()
                End If
            End If

            If Player_Turn = True Then
                If (Input_Place.Length = 5 Or Input_Place.Length = 6) And Amount_Of_The_Elements = 3 Then
                    Input_Place_Split(0) = Input_Place_Split(0).ToUpper
                    Input_Place_Split(2) = Input_Place_Split(2).ToUpper
                    If IsNumeric(Input_Place_Split(1)) And (New System.Text.RegularExpressions.Regex("^[a-jA-J]{1,}$")).IsMatch(Input_Place_Split(0)) Then
                        If Input_Place_Split(1) > 0 And Input_Place_Split(1) < 19 Then
                            If ((Input_Place_Split(2) = "U") Or (Input_Place_Split(2) = "D") Or (Input_Place_Split(2) = "R") Or (Input_Place_Split(2) = "L")) And Input_Place_Split(1) > 0 And Input_Place_Split(1) < 11 Then
                                Player_Case_Check = True
                            Else
                                Player_Case_Check = False
                            End If
                        Else
                            Player_Case_Check = False
                        End If
                    Else
                        Player_Case_Check = False
                    End If
                Else
                    Player_Case_Check = False
                End If

            End If
            Amount_Of_The_Elements = 0

            If Player_Turn = False Then
                Input_Place_Split(0) = Random_Letter()
                Input_Place_Split(0) = Input_Place_Split(0).ToUpper
                Input_Place_Split(1) = _random.Next(1, 11)
                Dim third_position = _random.Next(1, 5)

                If third_position = 1 Then
                    Input_Place_Split(2) = "U"
                End If
                If third_position = 2 Then
                    Input_Place_Split(2) = "D"
                End If
                If third_position = 3 Then
                    Input_Place_Split(2) = "R"
                End If
                If third_position = 4 Then
                    Input_Place_Split(2) = "L"
                End If
                Player_Case_Check = True
            End If

            If Player_Case_Check = True Then



                x = Input_Place_Split(0)
                X_Coordinate = Letter_to_Number(x)
                Console.WriteLine(X_Coordinate)

                If Field_General(X_Coordinate, Input_Place_Split(1)) = "░" Then



                    If Input_Place_Split(2) = "U" Then
                        If (Input_Place_Split(1) - (Ship_Szie - 1)) > 0 Then

                            If Field_General(X_Coordinate, Input_Place_Split(1) - (Ship_Szie - 1)) = "░" Then
                                Field_General(X_Coordinate, Input_Place_Split(1)) = Ship_Szie.ToString
                                For i = 0 To Ship_Szie - 1
                                    Y_Coordinate = Input_Place_Split(1) - i
                                    Field_General(X_Coordinate, Y_Coordinate) = Ship_Szie.ToString

                                    Call Draw_Ship_Zone()

                                Next
                                Counter_Ships = Counter_Ships - 1


                            Else
                                Did_Clash = True
                            End If
                        Else
                            Did_Clash = True
                        End If
                    End If

                    If Input_Place_Split(2) = "D" Then
                        If (Input_Place_Split(1) + (Ship_Szie - 1)) < 11 Then

                            If Field_General(X_Coordinate, Input_Place_Split(1) + (Ship_Szie - 1)) = "░" Then
                                Field_General(X_Coordinate, Input_Place_Split(1)) = Ship_Szie.ToString

                                For i = 0 To Ship_Szie - 1
                                    Y_Coordinate = Input_Place_Split(1) + i
                                    Field_General(X_Coordinate, Y_Coordinate) = Ship_Szie.ToString

                                    Call Draw_Ship_Zone()

                                Next
                                Counter_Ships = Counter_Ships - 1

                            Else
                                Did_Clash = True
                            End If
                        Else
                            Did_Clash = True
                        End If
                    End If


                    If Input_Place_Split(2) = "R" Then
                        If (X_Coordinate + (Ship_Szie - 1)) < 11 Then

                            If Field_General(X_Coordinate + (Ship_Szie - 1), Input_Place_Split(1)) = "░" Then
                                Field_General(X_Coordinate, Input_Place_Split(1)) = Ship_Szie.ToString
                                For i = 0 To Ship_Szie - 1
                                    If i < 1 Then
                                        X_Coordinate = X_Coordinate + i
                                    Else
                                        X_Coordinate = X_Coordinate + 1
                                    End If
                                    Y_Coordinate = Input_Place_Split(1)
                                    Field_General(X_Coordinate, Y_Coordinate) = Ship_Szie.ToString

                                    Call Draw_Ship_Zone()

                                Next
                                Counter_Ships = Counter_Ships - 1

                            Else
                                Did_Clash = True
                            End If
                        Else
                            Did_Clash = True
                        End If
                    End If




                    If Input_Place_Split(2) = "L" Then
                        If (X_Coordinate - (Ship_Szie - 1)) > 0 Then

                            If Field_General(X_Coordinate - (Ship_Szie - 1), Input_Place_Split(1)) = "░" Then
                                Field_General(X_Coordinate, Input_Place_Split(1)) = Ship_Szie.ToString
                                For i = 0 To Ship_Szie - 1
                                    If i < 1 Then
                                        X_Coordinate = X_Coordinate - i
                                    Else
                                        X_Coordinate = X_Coordinate - 1
                                    End If
                                    Y_Coordinate = Input_Place_Split(1)
                                    Field_General(X_Coordinate, Y_Coordinate) = Ship_Szie.ToString

                                    Call Draw_Ship_Zone()

                                Next
                                Counter_Ships = Counter_Ships - 1
                            Else
                                Did_Clash = True
                            End If
                        Else
                            Did_Clash = True
                        End If
                    End If



                    Console.WriteLine("Poistion is free")

                    If Counter_Ships = 9 And Did_Clash = False Then
                        Ship_Szie = Ship_Szie - 1
                    End If

                    If Counter_Ships = 7 And Did_Clash = False Then
                        Ship_Szie = Ship_Szie - 1
                    End If

                    If Counter_Ships = 4 And Did_Clash = False Then
                        Ship_Szie = Ship_Szie - 1
                    End If


                    If Player_Turn = True Then
                        If Did_Clash = True Then
                            Console.Beep(300, 80)
                            Console.Beep(260, 280)
                        Else
                            Console.Beep(600, 30)
                            Console.Beep(770, 250)
                        End If
                    End If


                    Call Refresh_Screen()
                    Call Draw_Fields()

                    Did_Clash = False

                End If

                Input_Check = True
                Console.WriteLine("Input is in correct format")
            Else

                ReDim Input_Place_Split(2)

            End If



        Loop


        Counter_Ships = 10




    End Sub

    Sub Draw_Ship_Zone()
        '''''''''''''''''''''''''''''''''''''''''''''''''''''

        If Field_General(X_Coordinate + 1, Y_Coordinate) = "░" Then
            Field_General(X_Coordinate + 1, Y_Coordinate) = "·"
        End If
        If Field_General(X_Coordinate + 1, Y_Coordinate + 1) = "░" Then
            Field_General(X_Coordinate + 1, Y_Coordinate + 1) = "·"
        End If
        If Field_General(X_Coordinate + 1, Y_Coordinate - 1) = "░" Then
            Field_General(X_Coordinate + 1, Y_Coordinate - 1) = "·"
        End If
        If Field_General(X_Coordinate, Y_Coordinate + 1) = "░" Then
            Field_General(X_Coordinate, Y_Coordinate + 1) = "·"
        End If
        If Field_General(X_Coordinate - 1, Y_Coordinate + 1) = "░" Then
            Field_General(X_Coordinate - 1, Y_Coordinate + 1) = "·"
        End If
        If Field_General(X_Coordinate - 1, Y_Coordinate) = "░" Then
            Field_General(X_Coordinate - 1, Y_Coordinate) = "·"
        End If
        If Field_General(X_Coordinate - 1, Y_Coordinate - 1) = "░" Then
            Field_General(X_Coordinate - 1, Y_Coordinate - 1) = "·"
        End If
        If Field_General(X_Coordinate, Y_Coordinate - 1) = "░" Then
            Field_General(X_Coordinate, Y_Coordinate - 1) = "·"
        End If
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    End Sub

    Sub Reveal()
        '''''''''''''''''''''''''''''''''''''''''''''''''''''
        Field_General(X_Coordinate + 1, Y_Coordinate) = Field_AI(X_Coordinate + 1, Y_Coordinate)
        Field_General(X_Coordinate + 1, Y_Coordinate + 1) = Field_AI(X_Coordinate + 1, Y_Coordinate + 1)
        Field_General(X_Coordinate + 1, Y_Coordinate - 1) = Field_AI(X_Coordinate + 1, Y_Coordinate - 1)
        Field_General(X_Coordinate, Y_Coordinate + 1) = Field_AI(X_Coordinate, Y_Coordinate + 1)
        Field_General(X_Coordinate - 1, Y_Coordinate + 1) = Field_AI(X_Coordinate - 1, Y_Coordinate + 1)
        Field_General(X_Coordinate - 1, Y_Coordinate) = Field_AI(X_Coordinate - 1, Y_Coordinate)
        Field_General(X_Coordinate - 1, Y_Coordinate - 1) = Field_AI(X_Coordinate - 1, Y_Coordinate - 1)
        Field_General(X_Coordinate, Y_Coordinate - 1) = Field_AI(X_Coordinate, Y_Coordinate - 1)
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    End Sub
    Sub Reveal_Ai()
        If Field_Player(Ai_X + 1, Ai_Y) = "░" Or Field_Player(Ai_X + 1, Ai_Y) = "·" Then
            Field_Player(Ai_X + 1, Ai_Y) = " "
        End If
        If Field_Player(Ai_X + 1, Ai_Y + 1) = "░" Or Field_Player(Ai_X + 1, Ai_Y + 1) = "·" Then
            Field_Player(Ai_X + 1, Ai_Y + 1) = " "
        End If
        If Field_Player(Ai_X + 1, Ai_Y - 1) = "░" Or Field_Player(Ai_X + 1, Ai_Y - 1) = "·" Then
            Field_Player(Ai_X + 1, Ai_Y - 1) = " "
        End If
        If Field_Player(Ai_X, Ai_Y + 1) = "░" Or Field_Player(Ai_X, Ai_Y + 1) = "·" Then
            Field_Player(Ai_X, Ai_Y + 1) = " "
        End If
        If Field_Player(Ai_X - 1, Ai_Y + 1) = "░" Or Field_Player(Ai_X - 1, Ai_Y + 1) = "·" Then
            Field_Player(Ai_X - 1, Ai_Y + 1) = " "
        End If
        If Field_Player(Ai_X - 1, Ai_Y) = "░" Or Field_Player(Ai_X - 1, Ai_Y) = "·" Then
            Field_Player(Ai_X - 1, Ai_Y) = " "
        End If
        If Field_Player(Ai_X - 1, Ai_Y - 1) = "░" Or Field_Player(Ai_X - 1, Ai_Y - 1) = "·" Then
            Field_Player(Ai_X - 1, Ai_Y - 1) = " "
        End If
        If Field_Player(Ai_X, Ai_Y - 1) = "░" Or Field_Player(Ai_X, Ai_Y - 1) = "·" Then
            Field_Player(Ai_X, Ai_Y - 1) = " "
        End If

    End Sub
    Sub Action()


        Dim Destroyed As Boolean = False
        Dim Destroyed_Ai As Boolean = False
        Dim Ai_did_hit As Boolean = False
        Dim Ai_did_Second_time As Boolean = False

        Dim Ai_X_o As Integer
        Dim Ai_Y_o As Integer

        Dim third_position As Integer = 0
        Dim Horizontal As Boolean = False
        Dim Vertical As Boolean = False
        Dim Check As Boolean = False
        Dim ship_size As Char

        Dim exceptions(3) As Integer

        Do Until Counter_Points_Ai = 20 Or Counter_Points_player = 20


            If Player_Turn = True Then


                Do Until Check = True
                    Console.WriteLine("Please input the target coordinates in the format of Letter Number")
                    Input_Place = Console.ReadLine

                    Dim Input_Place_Split() As String = Input_Place.Split(New Char() {" "c})


                    For Each item As Char In Input_Place_Split
                        Amount_Of_The_Elements = Amount_Of_The_Elements + 1
                    Next

                    If Amount_Of_The_Elements = 2 Then
                        Input_Place_Split(0) = Input_Place_Split(0).ToUpper


                        If IsNumeric(Input_Place_Split(1)) Then
                        Else
                            Input_Place_Split(0) = "A"
                            Input_Place_Split(1) = "15"
                        End If

                        If IsNumeric(Input_Place_Split(1)) And Input_Place_Split(1) < 11 And Input_Place_Split(1) > 0 And (New System.Text.RegularExpressions.Regex("^[a-jA-J]{1,}$")).IsMatch(Input_Place_Split(0)) Then


                            Check = True

                            x = Input_Place_Split(0)
                            X_Coordinate = Letter_to_Number(x)
                            Y_Coordinate = Input_Place_Split(1)
                        End If

                    End If

                    Amount_Of_The_Elements = 0

                Loop

                Check = False


                If Field_AI(X_Coordinate, Y_Coordinate) = "1" Or Field_AI(X_Coordinate, Y_Coordinate) = "2" Or Field_AI(X_Coordinate, Y_Coordinate) = "3" Or Field_AI(X_Coordinate, Y_Coordinate) = "4" Then

                    Counter_Points_player = Counter_Points_player + 1
                    Field_General(X_Coordinate, Y_Coordinate) = "X"

                    'Case when player hit 1 sized ship

                    If Field_AI(X_Coordinate, Y_Coordinate) = "1" Then
                        Call Reveal()
                        Field_General(X_Coordinate, Y_Coordinate) = "×"
                        Destroyed = True

                    End If

                    'Case when player hit 2 sized ship

                    If Field_AI(X_Coordinate, Y_Coordinate) = "2" Then

                        For i = 1 To 9
                            If Field_General(i, Y_Coordinate) = "X" And Field_General(i + 1, Y_Coordinate) = "X" And Field_AI(i, Y_Coordinate) = "2" And Field_AI(i + 1, Y_Coordinate) = "2" Then
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 1, Y_Coordinate) = "×"
                                Destroyed = True
                            End If
                        Next

                        For i = 1 To 9
                            If Field_General(X_Coordinate, i) = "X" And Field_General(X_Coordinate, i + 1) = "X" And Field_AI(X_Coordinate, i) = "2" And Field_AI(X_Coordinate, i + 1) = "2" Then
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 1) = "×"
                                Destroyed = True
                            End If
                        Next

                    End If

                    'Case when player hit 3 sized ship

                    If Field_AI(X_Coordinate, Y_Coordinate) = "3" Then

                        For i = 1 To 8
                            If Field_General(i, Y_Coordinate) = "X" And Field_General(i + 1, Y_Coordinate) = "X" And Field_General(i + 2, Y_Coordinate) = "X" And Field_AI(i, Y_Coordinate) = "3" And Field_AI(i + 1, Y_Coordinate) = "3" And Field_AI(i + 2, Y_Coordinate) = "3" Then
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 1, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 2, Y_Coordinate) = "×"
                                Destroyed = True
                            End If
                        Next

                        For i = 1 To 8
                            If Field_General(X_Coordinate, i) = "X" And Field_General(X_Coordinate, i + 1) = "X" And Field_General(X_Coordinate, i + 2) = "X" And Field_AI(X_Coordinate, i) = "3" And Field_AI(X_Coordinate, i + 1) = "3" And Field_AI(X_Coordinate, i + 2) = "3" Then
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 1) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 2) = "×"
                                Destroyed = True
                            End If
                        Next

                    End If

                    'Case when player hit 4 sized ship

                    If Field_AI(X_Coordinate, Y_Coordinate) = "4" Then

                        For i = 1 To 7
                            If Field_General(i, Y_Coordinate) = "X" And Field_General(i + 1, Y_Coordinate) = "X" And Field_General(i + 2, Y_Coordinate) = "X" And Field_General(i + 3, Y_Coordinate) = "X" And Field_AI(i, Y_Coordinate) = "4" And Field_AI(i + 1, Y_Coordinate) = "4" And Field_AI(i + 2, Y_Coordinate) = "4" And Field_AI(i + 3, Y_Coordinate) = "4" Then
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                X_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 1, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 2, Y_Coordinate) = "×"
                                Field_General(X_Coordinate - 3, Y_Coordinate) = "×"
                                Destroyed = True
                            End If
                        Next

                        For i = 1 To 7
                            If Field_General(X_Coordinate, i) = "X" And Field_General(X_Coordinate, i + 1) = "X" And Field_General(X_Coordinate, i + 2) = "X" And Field_General(X_Coordinate, i + 3) = "X" And Field_AI(X_Coordinate, i) = "4" And Field_AI(X_Coordinate, i + 1) = "4" And Field_AI(X_Coordinate, i + 2) = "4" And Field_AI(X_Coordinate, i + 3) = "4" Then
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                i = i + 1
                                Y_Coordinate = i
                                Call Reveal()
                                Field_General(X_Coordinate, Y_Coordinate) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 1) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 2) = "×"
                                Field_General(X_Coordinate, Y_Coordinate - 3) = "×"
                                Destroyed = True
                            End If
                        Next

                    End If

                Else

                    If Field_General(X_Coordinate, Y_Coordinate) = "░" Then
                        Field_General(X_Coordinate, Y_Coordinate) = "·"
                        Player_Turn = False
                    End If


                End If

                If Destroyed = True Then
                    Call Destroyed_animation_ship()
                    Call animationAI()
                End If

                Destroyed = False


            End If




            'AI's TURN///////////////////////////////////////////////////////////////// DEBUG!!!!!



            If Player_Turn = False Then


                If Ai_did_hit = False Then

                    Ai_X = _random.Next(1, 11)
                    Ai_Y = _random.Next(1, 11)


                    If Field_Player(Ai_X, Ai_Y) <> "·" And Field_Player(Ai_X, Ai_Y) <> " " And Field_Player(Ai_X, Ai_Y) <> "░" And Field_Player(Ai_X, Ai_Y) <> "X" And Field_Player(Ai_X, Ai_Y) <> "×" Then


                        ship_size = Field_Player(Ai_X, Ai_Y)
                        Counter_Points_Ai = Counter_Points_Ai + 1

                        Field_Player(Ai_X, Ai_Y) = "X"
                        Ai_did_hit = True

                    Else
                        If Field_Player(Ai_X, Ai_Y) = "·" Or Field_Player(Ai_X, Ai_Y) = "░" Then
                            Player_Turn = True
                            Field_Player(Ai_X, Ai_Y) = " "
                        End If
                    End If

                End If



                If Ai_did_hit = True And Ai_did_Second_time = False Then

                    Do Until exceptions(0) <> third_position And exceptions(1) <> third_position And exceptions(2) <> third_position And exceptions(3) <> third_position

                        third_position = _random.Next(1, 5)

                    Loop

                    If third_position = 1 And Ai_X + 1 < 11 Then
                        If Field_Player(Ai_X + 1, Ai_Y) <> "·" And Field_Player(Ai_X + 1, Ai_Y) <> " " And Field_Player(Ai_X + 1, Ai_Y) <> "░" Then

                            Ai_X_o = Ai_X
                            Ai_Y_o = Ai_Y



                            Field_Player(Ai_X + 1, Ai_Y) = "X"
                            Counter_Points_Ai = Counter_Points_Ai + 1
                            Ai_X = Ai_X + 1
                            Horizontal = True
                            Ai_did_Second_time = True

                        Else

                            If Field_Player(Ai_X + 1, Ai_Y) <> " " Then
                                Field_Player(Ai_X + 1, Ai_Y) = " "
                                Player_Turn = True
                            End If

                            exceptions(0) = 1
                        End If
                    End If

                    If Ai_X + 1 > 10 Then
                        exceptions(0) = 1
                    End If

                    If third_position = 2 And Ai_X - 1 > 0 Then
                        If Field_Player(Ai_X - 1, Ai_Y) <> "·" And Field_Player(Ai_X - 1, Ai_Y) <> " " And Field_Player(Ai_X - 1, Ai_Y) <> "░" Then

                            Ai_X_o = Ai_X
                            Ai_Y_o = Ai_Y



                            Field_Player(Ai_X - 1, Ai_Y) = "X"
                            Counter_Points_Ai = Counter_Points_Ai + 1
                            Ai_X = Ai_X - 1
                            Horizontal = True
                            Ai_did_Second_time = True
                        Else

                            If Field_Player(Ai_X - 1, Ai_Y) <> " " Then
                                Field_Player(Ai_X - 1, Ai_Y) = " "
                                Player_Turn = True
                            End If

                            exceptions(1) = 2

                        End If
                    End If

                    If Ai_X - 1 < 1 Then
                        exceptions(1) = 2
                    End If

                    If third_position = 3 And Ai_Y + 1 < 11 Then
                        If Field_Player(Ai_X, Ai_Y + 1) <> "·" And Field_Player(Ai_X, Ai_Y + 1) <> " " And Field_Player(Ai_X, Ai_Y + 1) <> "░" Then

                            Ai_X_o = Ai_X
                            Ai_Y_o = Ai_Y



                            Field_Player(Ai_X, Ai_Y + 1) = "X"
                            Counter_Points_Ai = Counter_Points_Ai + 1
                            Ai_Y = Ai_Y + 1
                            Vertical = True
                            Ai_did_Second_time = True
                        Else

                            If Field_Player(Ai_X, Ai_Y + 1) <> " " Then
                                Field_Player(Ai_X, Ai_Y + 1) = " "
                                Player_Turn = True
                            End If

                            exceptions(2) = 3
                        End If
                    End If

                    If Ai_Y + 1 > 10 Then
                        exceptions(2) = 3
                    End If

                    If third_position = 4 And Ai_Y - 1 > 0 Then
                        If Field_Player(Ai_X, Ai_Y - 1) <> "·" And Field_Player(Ai_X, Ai_Y - 1) <> " " And Field_Player(Ai_X, Ai_Y - 1) <> "░" Then

                            Ai_X_o = Ai_X
                            Ai_Y_o = Ai_Y



                            Field_Player(Ai_X, Ai_Y - 1) = "X"
                            Counter_Points_Ai = Counter_Points_Ai + 1
                            Ai_Y = Ai_Y - 1
                            Vertical = True
                            Ai_did_Second_time = True
                        Else

                            If Field_Player(Ai_X, Ai_Y - 1) <> " " Then
                                Field_Player(Ai_X, Ai_Y - 1) = " "
                                Player_Turn = True
                            End If

                            exceptions(3) = 4
                        End If
                    End If

                    If Ai_Y - 1 < 1 Then
                        exceptions(3) = 4
                    End If

                End If




                If Ai_did_Second_time = True Then
                    If third_position = 1 And Ai_X + 1 < 11 And Player_Turn = False Then
                        If Field_Player(Ai_X + 1, Ai_Y) <> "·" And Field_Player(Ai_X + 1, Ai_Y) <> " " And Field_Player(Ai_X + 1, Ai_Y) <> "░" Then
                            Field_Player(Ai_X + 1, Ai_Y) = "X"

                            Counter_Points_Ai = Counter_Points_Ai + 1

                            If Field_Player(Ai_X + 1, Ai_Y) = "X" Then
                                Ai_X = Ai_X + 1
                            Else
                                Field_Player(Ai_X + 1, Ai_Y) = "X"
                            End If

                        Else
                            third_position = 2
                            Ai_X = Ai_X_o
                            Player_Turn = True
                        End If
                    Else

                        If Ai_X + 1 > 10 And third_position = 1 Then
                            third_position = 2
                            Ai_X = Ai_X_o
                        End If

                    End If
                    If third_position = 2 And Ai_X - 1 > 0 And Player_Turn = False Then
                        If Field_Player(Ai_X - 1, Ai_Y) <> "·" And Field_Player(Ai_X - 1, Ai_Y) <> " " And Field_Player(Ai_X - 1, Ai_Y) <> "░" Then
                            Field_Player(Ai_X - 1, Ai_Y) = "X"

                            Counter_Points_Ai = Counter_Points_Ai + 1

                            If Field_Player(Ai_X - 1, Ai_Y) = "X" Then
                                Ai_X = Ai_X - 1
                            Else
                                Field_Player(Ai_X - 1, Ai_Y) = "X"
                            End If

                        Else
                            third_position = 1
                            Ai_X = Ai_X_o
                            Player_Turn = True
                        End If
                    Else

                        If Ai_X - 1 < 1 And third_position = 2 Then
                            third_position = 1
                            Ai_X = Ai_X_o
                        End If

                    End If

                    If third_position = 3 And Ai_Y + 1 < 11 And Player_Turn = False Then
                        If Field_Player(Ai_X, Ai_Y + 1) <> "·" And Field_Player(Ai_X, Ai_Y + 1) <> " " And Field_Player(Ai_X, Ai_Y + 1) <> "░" Then

                            Counter_Points_Ai = Counter_Points_Ai + 1

                            If Field_Player(Ai_X, Ai_Y + 1) = "X" Then
                                Ai_Y = Ai_Y + 1
                            Else
                                Field_Player(Ai_X, Ai_Y + 1) = "X"
                            End If

                        Else
                            third_position = 4
                            Ai_Y = Ai_Y_o
                            Player_Turn = True
                        End If
                    Else

                        If Ai_Y + 1 > 10 And third_position = 3 Then
                            third_position = 4
                            Ai_Y = Ai_Y_o
                        End If

                    End If

                    If third_position = 4 And Ai_Y - 1 > 0 And Player_Turn = False Then
                        If Field_Player(Ai_X, Ai_Y - 1) <> "·" And Field_Player(Ai_X, Ai_Y - 1) <> " " And Field_Player(Ai_X, Ai_Y - 1) <> "░" Then
                            Field_Player(Ai_X, Ai_Y - 1) = "X"

                            Counter_Points_Ai = Counter_Points_Ai + 1

                            If Field_Player(Ai_X, Ai_Y - 1) = "X" Then
                                Ai_Y = Ai_Y - 1
                            Else
                                Field_Player(Ai_X, Ai_Y - 1) = "X"
                            End If

                        Else
                            third_position = 3
                            Ai_Y = Ai_Y_o
                            Player_Turn = True
                        End If
                    Else

                        If Ai_Y - 1 < 1 And third_position = 4 Then
                            third_position = 3
                            Ai_Y = Ai_Y_o
                        End If

                    End If

                End If
            End If


            ' 1

            If ship_size = "1" Then
                Call Reveal_Ai()
                Field_Player(Ai_X, Ai_Y) = "×"
                Destroyed_Ai = True

            End If

            '2 

            If ship_size = "2" Then

                For i = 1 To 9
                    If Field_Player(i, Ai_Y) = "X" And Field_Player(i + 1, Ai_Y) = "X" Then
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X - 1, Ai_Y) = "×"
                        Destroyed_Ai = True
                    End If
                Next

                For i = 1 To 9
                    If Field_Player(Ai_X, i) = "X" And Field_Player(Ai_X, i + 1) = "X" Then
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X, Ai_Y - 1) = "×"
                        Destroyed_Ai = True
                    End If
                Next

            End If

            'Case when player hit 3 sized ship

            If ship_size = "3" Then

                For i = 1 To 8
                    If Field_Player(i, Ai_Y) = "X" And Field_Player(i + 1, Ai_Y) = "X" And Field_Player(i + 2, Ai_Y) = "X" Then
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X - 1, Ai_Y) = "×"
                        Field_Player(Ai_X - 2, Ai_Y) = "×"
                        Destroyed_Ai = True
                    End If
                Next

                For i = 1 To 8
                    If Field_Player(Ai_X, i) = "X" And Field_Player(Ai_X, i + 1) = "X" And Field_Player(Ai_X, i + 2) = "X" Then
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X, Ai_Y - 1) = "×"
                        Field_Player(Ai_X, Ai_Y - 2) = "×"
                        Destroyed_Ai = True
                    End If
                Next

            End If

            'Case when player hit 4 sized ship

            If ship_size = "4" Then

                For i = 1 To 7
                    If Field_Player(i, Ai_Y) = "X" And Field_Player(i + 1, Ai_Y) = "X" And Field_Player(i + 2, Ai_Y) = "X" And Field_Player(i + 3, Ai_Y) = "X" Then
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_X = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X - 1, Ai_Y) = "×"
                        Field_Player(Ai_X - 2, Ai_Y) = "×"
                        Field_Player(Ai_X - 3, Ai_Y) = "×"
                        Destroyed_Ai = True
                    End If
                Next

                For i = 1 To 7
                    If Field_Player(Ai_X, i) = "X" And Field_Player(Ai_X, i + 1) = "X" And Field_Player(Ai_X, i + 2) = "X" And Field_Player(Ai_X, i + 3) = "X" Then
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        i = i + 1
                        Ai_Y = i
                        Call Reveal_Ai()
                        Field_Player(Ai_X, Ai_Y) = "×"
                        Field_Player(Ai_X, Ai_Y - 1) = "×"
                        Field_Player(Ai_X, Ai_Y - 2) = "×"
                        Field_Player(Ai_X, Ai_Y - 3) = "×"
                        Destroyed_Ai = True
                    End If
                Next

            End If



            If Destroyed_Ai = True Then
                Console.WriteLine("Ship was destroyed")

                Call Destroyed_animation_ship()
                Call animationUSER()

                Ai_did_hit = False
                Ai_did_Second_time = False
                Horizontal = False
                Vertical = False
                third_position = 0

                ship_size = "0"

                ReDim exceptions(3)
                Player_Turn = False
            End If

            Destroyed_Ai = False

            Call Refresh_Screen()

            Call Draw_Fields()


        Loop



        If Counter_Points_player = 20 Then
            Call Refresh_Screen()
            Call Refresh_Screen()
            Console.WriteLine("
                                                                              ██╗   ██╗ ██████╗ ██╗   ██╗    ██╗    ██╗ ██████╗ ███╗   ██╗██╗
                                                                              ╚██╗ ██╔╝██╔═══██╗██║   ██║    ██║    ██║██╔═══██╗████╗  ██║██║
                                                                               ╚████╔╝ ██║   ██║██║   ██║    ██║ █╗ ██║██║   ██║██╔██╗ ██║██║
                                                                                ╚██╔╝  ██║   ██║██║   ██║    ██║███╗██║██║   ██║██║╚██╗██║╚═╝
                                                                                 ██║   ╚██████╔╝╚██████╔╝    ╚███╔███╔╝╚██████╔╝██║ ╚████║██╗
                                                                                 ╚═╝    ╚═════╝  ╚═════╝      ╚══╝╚══╝  ╚═════╝ ╚═╝  ╚═══╝╚═╝
                                                               
")
            Call Refresh_Screen()

        End If

        If Counter_Points_Ai = 20 Then
            Call Refresh_Screen()
            Call Refresh_Screen()
            Console.WriteLine("
                                                                                  ██╗   ██╗ ██████╗ ██╗   ██╗    ██╗      ██████╗ ███████╗████████╗██╗
                                                                                  ╚██╗ ██╔╝██╔═══██╗██║   ██║    ██║     ██╔═══██╗██╔════╝╚══██╔══╝██║
                                                                                   ╚████╔╝ ██║   ██║██║   ██║    ██║     ██║   ██║███████╗   ██║   ██║
                                                                                    ╚██╔╝  ██║   ██║██║   ██║    ██║     ██║   ██║╚════██║   ██║   ╚═╝
                                                                                     ██║   ╚██████╔╝╚██████╔╝    ███████╗╚██████╔╝███████║   ██║   ██╗
                                                                                     ╚═╝    ╚═════╝  ╚═════╝     ╚══════╝ ╚═════╝ ╚══════╝   ╚═╝   ╚═╝
                                                                    
")
            Call Refresh_Screen()

        End If

    End Sub

    Sub Main()
        Console.WriteLine("Open FullScreen and press any key")
        Console.ReadKey()

        Call Start_Menu()

        ' Call animationship()

        Call Refresh_Screen()
        Call Ini_Field()

        For i = 0 To 11
            For j = 0 To 11
                Field_Player(i, j) = Field_General(i, j)
                Field_AI(i, j) = Field_General(i, j)
            Next
        Next


        Call Draw_Fields()


        SetConsoleColors(ConsoleColor.white)

        Call Place_Ships()

        Call Copy_Fields()
        Player_Turn = False

        Call Refresh_Screen()
        Call Draw_Fields()

        Call Ini_Field()

        ' AI places boats

        Call Place_Ships()

        Call Copy_Fields()

        Call Ini_Field()

        Ships_Placed = True
        X_Coordinate = 0
        Y_Coordinate = 0
        Ai_X = 0
        Ai_Y = 0

        Call Draw_Fields()



        'Game starts
        Player_Turn = True
        Ships_Placed = True


        Call Action()


        Console.ReadKey()
    End Sub

    Function Letter_to_Number(x) As Integer
        Dim num As Integer
        num = Asc(x) - 64
        Return num
    End Function


    Function Random_Letter() As Char

        ' Get random number between 0 and 25 inclusive.
        ' ... The second argument 26 is exclusive.
        ' ... This corresponds to an ASCII char.
        Dim number As Integer = _random.Next(0, 11)
        ' Convert lowercase "a" to an Integer with Asc.
        ' ... Use this value to offset the number we generated.
        ' ... Convert back to a Char with Chr.
        Dim letter As Char = Chr((Asc("a"c) + number))

        Return letter
    End Function

End Module


